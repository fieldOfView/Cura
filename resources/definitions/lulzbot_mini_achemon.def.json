{
    "id": "lulzbot_mini_achemon",
    "name": "LulzBot Mini Aerostruder v2 Micro",
    "inherits": "lulzbot_mini",
    "version": 2,
    "metadata":
    {
        "machine_extruder_trains":
        {
            "0": "lulzbot_mini_achemon_extruder"
        },
        "type": "machine",
        "visible": true,
        "user_toolhead_name": "Aerostruder v2 Micro",
        "firmware_toolhead_name": "AerostruderMicro"
    },
    "overrides":
    {
        "machine_name":
        {
            "default_value": "LulzBot Mini Aerostruder v2 Micro"
        },
        "machine_start_gcode":
        {
            "default_value_from_file": "lulzbot_mini_achemon_start"
        },
        "machine_end_gcode":
        {
            "default_value_from_file": "lulzbot_mini_end"
        }
    }
}
